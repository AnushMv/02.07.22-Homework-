function getOddEven(){
    let evenArr = [];
    let oddArr = [];
    for(let i = 0, j = 0, k = 0; i < arr.length; i++){
        if(arr[i] % 2 === 0){
            evenArr[j] = arr[i];
            j++;
        } else{
           oddArr[k] = arr[i];
            k++;
        }
    }
    return [evenArr, oddArr];
}
