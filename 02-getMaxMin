function getMaxMin(arr){
    let max = arr[0];
    let min = arr[0];
    
    for(let i = 1; i < arr.length; i++){
        if(max < arr[i]){
            max = arr[i];
        
        } else if(min > arr[i]){
             min = arr[i];
         }    
    }
    let newArr = [];
    newArr[0] = min;
    newArr[1] = max;
    return newArr;
}
