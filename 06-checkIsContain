function checkIsContain(arr1,arr2){
    let newArr = [];
    for(let i = 0, k= 0; i < arr1.length; i++){
        for(let j = 0; j < arr2.length; j++){
            if(arr1[i] === arr2[j]){
                newArr[k] = arr1[i];
                k++;
            }
        }
    }
    if(newArr.length === arr1.length){
        return true;
    } else{
        return false;
    }
}
