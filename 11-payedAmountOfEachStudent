function getSumOfEachpayment(payments){
    let sum = 0;
    let arrOfSum = [];
    for(let i = 0, k =0; i < payments.length; i++){
        if(!payments[i].studentId){
            sum = payments[i].payedAmount;
        } else {
            sum = sum + payments[i].payedAmount;
            k++;
        }
        arrOfSum[i] = {
          studentId: payments[k].studentId,
          studentName: payments[k].studentName,
          payedAmount: sum,
          
        }
    }
return arrOfSum;
}
