function getCommon(arr1,arr2){
    let commonArr = [];
    for(let i = 0, k = 0; i < arr1.length; i++){
        for(let j = 0; j < arr2.length; j++){
            if(arr1[i] === arr2[j]){
                commonArr[k] = arr1[i];
                k++;
            }
        }
    }
    return commonArr;
}
